    "mustache": "^4.2.0",
    "openapi-client-axios": "^7.5.5",
    "openapi-schema-validator": "^12.1.3",
    "openapi-types": "^12.1.3",
    "which": "^5.0.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.17.16",
