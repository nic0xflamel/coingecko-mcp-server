    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "description": "MCP server for the CoinGecko API (Public)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nic0flamel/notion-automation.git"
  },
